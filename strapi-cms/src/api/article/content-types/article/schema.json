{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "richtext",
      "required": true
    },
    "Content": {
      "type": "richtext",
      "required": true
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "articles"
    },
    "Slug": {
      "type": "uid",
      "required": true,
      "targetField": "Title"
    }
  }
}
